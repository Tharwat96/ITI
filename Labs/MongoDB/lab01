6. 

db.createCollection("users", 
    {capped:true, size:5242880, max:10, validator:
        {$and:
            [{username:{$type:"string"}},
            {email:{$regex:/@gmail\.com$/}}
            ]
        }
    }
)

db.createCollection( "contacts",
   { validator: { $or:
      [
         { phone: { $type: "string" } },
         { email: { $regex: /@mongodb\.com$/ } },
         { status: { $in: [ "Unknown", "Incomplete" ] } }
      ]
   }
} )

