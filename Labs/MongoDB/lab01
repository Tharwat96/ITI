for(let i = 2; i<22; i++){
    db.posts.insert({_id:i, "username": "Mohamed", "name":`post${i}`, "likes":`${i}`, "comments": i})
}

db.posts.insert({_id:50, "username": "Ahmed", "name":"post50", "likes": 16, "comments": "yest"})


